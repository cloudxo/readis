<table class="table table-striped table-responsive table-hover">
    <thead>
    <tr>
        <th>{{ keyCaption }}</th>
        <th>{{ valueCaption }}</th>
    </tr>
    </thead>
    <tbody>
    {% for key, value in keyValues %}
        <tr>
            <td>
                {{ key }}
            </td>
            <td>
                {{ value }}
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>